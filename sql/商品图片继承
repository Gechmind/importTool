INSERT INTO `temp_product`.`merchant_picture` (
	`id`,
	`merchant_id`,
	`picture_id`,
	`sort_value`,
	`is_available`,
	`is_deleted`,
	`version_no`,
	`create_userid`,
	`create_username`,
	`create_userip`,
	`create_usermac`,
	`create_time`,
	`create_time_db`,
	`server_ip`,
	`update_userid`,
	`update_username`,
	`update_userip`,
	`update_usermac`,
	`update_time`,
	`update_time_db`,
	`client_versionno`,
	`company_id`
)
select 
		NULL,
		'101',
		id,
		sort_value,
		is_available,
		is_deleted,
		version_no,
	`create_userid`,
	`create_username`,
	`create_userip`,
	`create_usermac`,
	`create_time`,
	`create_time_db`,
	`server_ip`,
	`update_userid`,
	`update_username`,
	`update_userip`,
	`update_usermac`,
	`update_time`,
	`update_time_db`,
	`client_versionno`,
		1003
   from product_picture;



INSERT INTO `temp_product`.`merchant_prod_picture` (
	`id`,
	`merchant_prod_id`,
	`merchant_picture_id`,
	`type`,
	`is_available`,
	`is_deleted`,
	`version_no`,
	`create_userid`,
	`create_username`,
	`create_userip`,
	`create_usermac`,
	`create_time`,
	`create_time_db`,
	`server_ip`,
	`update_userid`,
	`update_username`,
	`update_userip`,
	`update_usermac`,
	`update_time`,
	`update_time_db`,
	`client_versionno`,
	`company_id`,
	`sort_value`
)
SELECT
	null,
  p.id as merchant_product_id,
  t.id as merchant_picture_id, 
  0,
  t.is_available,
	t.is_deleted,
	t.version_no,
	t.create_userid,
	t.create_username,
	t.create_userip,
	t.create_usermac,
	t.create_time,
	t.create_time_db,
	t.server_ip,
	t.update_userid,
	t.update_username,
	t.update_userip,
	t.update_usermac,
	t.update_time,
	t.update_time_db,
	t.client_versionno,
   1003,
   1
FROM
	merchant_picture t,
	product_picture p,
	merchant_product x
WHERE
	t.picture_id = p.id
AND p.product_id = x.product_id;